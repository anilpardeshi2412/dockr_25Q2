pipeline {
			agent {
					label {
								label "built-in"
								customWorkspace "/mnt/dock2"
							}
				}		
	stages {
		
		stage ('docker httpd') {
			
				steps {
			
				sh '''
						docker run -dp 90:80 --name s2 httpd
						docker cp /mnt/dock1/index.html s2:/usr/local/apache2/htdocs/								
					
					'''
					}			
				}	
					
			}		
}
		
